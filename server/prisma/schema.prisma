generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model YoutubeVideo {
  id       Int      @id @default(autoincrement())
  name     String
  date     DateTime
  link     String   @unique
  location String?
}
